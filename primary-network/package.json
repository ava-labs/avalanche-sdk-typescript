{
  "name": "@avalanche-sdk/primary-network",
  "version": "0.0.4",
  "type": "module",
  "description": "Primary Network module for building and interacting with Avalanche ecosystem",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx vitest run",
    "test:cov": "npx vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ava-labs/avalanche-sdk-typescript.git",
    "directory": "primary-network"
  },
  "files": [
    "dist",
    "src",
    "!src/**/*.test.ts",
    "!src/**/fixtures/**"
  ],
  "keywords": [
    "primary-network",
    "p-chain",
    "x-chain"
  ],
  "author": "Raj Ranjan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ava-labs/avalanche-sdk-typescript/issues"
  },
  "homepage": "https://github.com/ava-labs/avalanche-sdk-typescript/tree/main/primary-network#readme",
  "dependencies": {
    "@avalabs/avalanchejs": "5.1.0-alpha.1",
    "@avalanche-sdk/rpc": "^0.0.4-alpha.1",
    "@noble/hashes": "^1.8.0",
    "@scure/base": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.1.4",
    "typescript": "^5.0.0",
    "vitest": "^3.1.4"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./warp": "./dist/warp/index.js",
    "./transactions/pchain": "./dist/transactions/p-chain/index.js"
  },
  "typesVersions": {
    "*": {
      "warp": [
        "dist/warp/index.d.ts"
      ],
      "transactions/pchain": [
        "dist/transactions/p-chain/index.d.ts"
      ],
      "transactions/cchain": [
        "dist/transactions/c-chain/index.d.ts"
      ]
    }
  }
}
