{
  "name": "@avalanche-sdk/sdk",
  "version": "1.2.1",
  "author": "Sayan Kar",
  "license": "ISC",
  "homepage": "https://github.com/ava-labs/avalanche-sdk-typescript/sdk#readme",
  "sideEffects": false,
  "scripts": {
    "build": "tshy",
    "lint": "eslint --cache --max-warnings=0 src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ava-labs/avalanche-sdk-typescript.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/ava-labs/avalanche-sdk-typescript/issues"
  },
  "dependencies": {
    "@avalanche-sdk/data": "^0.2.2",
    "@avalanche-sdk/metrics": "^0.3.0",
    "@avalanche-sdk/webhooks": "^0.3.1",
    "eslint": "^9.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "ts-node": "^10.9.2",
    "tshy": "^3.0.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.22.0"
  },
  "tshy": {
    "sourceDialects": [
      "@avalanche-sdk/sdk/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./models/errors": "./src/models/errors/index.ts",
      "./data": "./src/data/index.ts",
      "./data/types": "./src/data/types/index.ts",
      "./data/models/errors": "./src/data/models/errors/index.ts",
      "./data/models/components": "./src/data/models/components/index.ts",
      "./data/models/operations": "./src/data/models/operations/index.ts",
      "./metrics": "./src/metrics/index.ts",
      "./metrics/types": "./src/metrics/types/index.ts",
      "./metrics/models/errors": "./src/metrics/models/errors/index.ts",
      "./metrics/models/components": "./src/metrics/models/components/index.ts",
      "./metrics/models/operations": "./src/metrics/models/operations/index.ts",
      "./webhooks": "./src/webhooks/index.ts",
      "./webhooks/types": "./src/webhooks/types/index.ts",
      "./webhooks/models/errors": "./src/webhooks/models/errors/index.ts",
      "./webhooks/models/components": "./src/webhooks/models/components/index.ts",
      "./webhooks/models/operations": "./src/webhooks/models/operations/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "exports": {
    ".": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./models/errors": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/models/errors/index.ts",
        "types": "./dist/esm/models/errors/index.d.ts",
        "default": "./dist/esm/models/errors/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/errors/index.d.ts",
        "default": "./dist/commonjs/models/errors/index.js"
      }
    },
    "./data": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/data/index.ts",
        "types": "./dist/esm/data/index.d.ts",
        "default": "./dist/esm/data/index.js"
      },
