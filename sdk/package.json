    },
    "./metrics/models/errors": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/metrics/models/errors/index.ts",
        "types": "./dist/esm/metrics/models/errors/index.d.ts",
        "default": "./dist/esm/metrics/models/errors/index.js"
      },
      "require": {
        "types": "./dist/commonjs/metrics/models/errors/index.d.ts",
        "default": "./dist/commonjs/metrics/models/errors/index.js"
      }
    },
    "./metrics/models/components": {
