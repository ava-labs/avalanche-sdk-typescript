        "@avalanche-sdk/sdk/source": "./src/data/models/errors/index.ts",
        "types": "./dist/esm/data/models/errors/index.d.ts",
        "default": "./dist/esm/data/models/errors/index.js"
      },
      "require": {
        "types": "./dist/commonjs/data/models/errors/index.d.ts",
        "default": "./dist/commonjs/data/models/errors/index.js"
      }
    },
    "./data/models/components": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/data/models/components/index.ts",
