{
  "name": "@avalanche-sdk/sdk",
  "version": "1.2.3",
  "author": "Sayan Kar",
  "license": "ISC",
  "homepage": "https://github.com/ava-labs/avalanche-sdk-typescript/sdk#readme",
  "sideEffects": false,
  "scripts": {
    "build": "tshy",
    "lint": "eslint --cache --max-warnings=0 src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ava-labs/avalanche-sdk-typescript.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/ava-labs/avalanche-sdk-typescript/issues"
  },
  "dependencies": {
    "@avalanche-sdk/devtools": "^0.0.5",
    "eslint": "^9.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "ts-node": "^10.9.2",
    "tshy": "^3.0.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.22.0"
  },
  "tshy": {
    "sourceDialects": [
      "@avalanche-sdk/sdk/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./types": "./src/types/index.ts",
      "./models/errors": "./src/models/errors/index.ts",
      "./models/components": "./src/models/components/index.ts",
      "./models/operations": "./src/models/operations/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "exports": {
    ".": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./types": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/types/index.ts",
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/types/index.js"
      },
      "require": {
        "types": "./dist/commonjs/types/index.d.ts",
        "default": "./dist/commonjs/types/index.js"
      }
    },
    "./models/errors": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/models/errors/index.ts",
        "types": "./dist/esm/models/errors/index.d.ts",
        "default": "./dist/esm/models/errors/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/errors/index.d.ts",
        "default": "./dist/commonjs/models/errors/index.js"
      }
    },
    "./models/components": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/models/components/index.ts",
        "types": "./dist/esm/models/components/index.d.ts",
        "default": "./dist/esm/models/components/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/components/index.d.ts",
        "default": "./dist/commonjs/models/components/index.js"
      }
    },
    "./models/operations": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/models/operations/index.ts",
        "types": "./dist/esm/models/operations/index.d.ts",
        "default": "./dist/esm/models/operations/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/operations/index.d.ts",
        "default": "./dist/commonjs/models/operations/index.js"
      }
    },
    "./*.js": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    },
    "./*": {
      "import": {
        "@avalanche-sdk/sdk/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "type": "module"
}